#ifndef ${id ${upcase $fn}}
#define ${id ${upcase $fn}}

#pragma interface

class ${id ${b $fn .h}}
{
public:
	${id ${b $fn .h}}();
	${id ${b $fn .h}}(const ${id ${b $fn .h}} &);
	${id ${b $fn .h}} operator=(const ${id ${b $fn .h}} &);
	~${id ${b $fn .h}}();
